{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  //"gtk-layer-shell": "false",
  "height": 46, // Waybar height (to be removed for auto height)
  // "width": 2560, // Waybar width
  "spacing": 0, // Gaps between modules (0px) Adjusted in the css
  "margin-top": 0,
  //"margin-bottom":-10,
  "margin-left": 10,
  "margin-right": 10,
  // Choose the order of the modules
  "modules-left": [
    "custom/launcher",
    "custom/updates",
    "cpu",
    "custom/gpu",
    "memory",
    "temperature",
    "disk",
    "network"
  ],
  "modules-center": [
    "hyprland/workspaces",
    "hyprland/window"
    // "hyprland/language"
  ],
  "modules-right": [
    // "custom/hyprshade",
    "pulseaudio",
    "backlight",
    // "battery",
    "upower",
    "custom/notifications",
    "tray",
    "clock",
    "custom/weather",
    // "custom/cycle_wall",
    // "custom/clipboard",
    "custom/power",
    "cava"
    // "custom/custom"
  ],
  "custom/launcher": {
    "format": "{}",
    "tooltip": true,
    "exec": "echo '{\"text\":\"ğŸ’§\",\"tooltip\":\"Drun | Run\"}'",
    "return-type": "json",
    "on-click": "~/.config/hypr/scripts/rofilaunch.sh d",
    "on-click-right": "~/.config/hypr/scripts/rofilaunch.sh w"
  },
  "custom/updates": {
    // "format": "ğŸ“¦ {}",
    "interval": 7200, // every two hours
    "format": "ó±§˜ {}",
    //"exec": "i=$(checkupdates); echo \"$i\" |wc -l; echo \"$i\" |column -t |tr '\n' '\r'",       // # of updates and tooltip details
    "exec": "~/.config/waybar/scripts/arch_updates.sh",
    "on-click": "kitty --class wm-floating --title all_is_kitty --hold --detach sh -c 'pacman -Syu'",
    "tooltip": true
  },
  "cpu": {
    "interval": 1,
    // "format": "ó°› {usage}%",
    // "format-alt": "{icon0}{icon1}{icon2}{icon3}",
    // "format-icons": ["â–", "â–‚", "â–ƒ", "â–„", "â–…", "â–†", "â–‡", "â–ˆ"],
    // "tooltip": true,
    "format": "ó°Œ¢ {load}",
    "tooltip": false,
    "on-click": "missioncenter",
    "on-click-right": "kitty --class wm-floating --title all_is_kitty --hold --detach sh -c 'btop'"
  },
  "custom/gpu": {
    "interval": 1,
    "exec": "nvidia-smi --query-gpu=utilization.gpu --format=csv,noheader,nounits",
    "format": "ó°¢® {}%",
    "return-type": "",
    "on-click": "missioncenter",
    "on-click-right": "kitty --class wm-floating --title all_is_kitty --hold --detach sh -c 'nvidia-smi'"
  },
  "memory": {
    "interval": 2,
    "format": "ó°¾… {used}GB",
    // "format-alt": "ó°¾† {percentage}%",
    "max-length": 30,
    "tooltip": true,
    "tooltip-format": "î‰¦ {used:0.1f}GB/{total:0.1f}GB",
    "on-click": "missioncenter",
    "on-click-right": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'"
  },
  "temperature": {
    "interval": 2,
    "format": "{temperatureC}Â°C ï‹‰",
    "format-icons": ["ï‹‹", "ï‹Š", "ï‹‰", "ï‹ˆ", "ï‹‡"]
  },
  "disk": {
    // "format": "ï‚  {percentage_used}% ({free})",
    // "format": "ğŸ’¿ {free}",
    "format": "ï‚  {free}",
    "format-alt": "ï‚  {percentage_used}% ({free})",
    "tooltip": true,
    "on-click-right": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'ncdu'",
    // "on-click-right": "kitty --start-as=fullscreen --title all_is_kitty sh -c 'btop'",
    "interval": 10
  },
  "network": {
    // "format": "â†•ï¸{bandwidthTotalBytes}",
    "format": "ó°¹¹{bandwidthTotalBytes}",
    "format-disconnected": "No Internetâš¡",
    "format-linked": "{ifname} (No IP) â€¼ï¸",
    // "format-alt": "â¬†ï¸ {bandwidthUpBytes} | â¬‡ï¸ {bandwidthDownBytes}",
    "format-alt": "ï¢ {bandwidthUpBytes} | ï£ {bandwidthDownBytes}",
    // "format-wifi": "{essid}({signalStrength}%) ğŸ›œ",
    "format-wifi": "{essid}({signalStrength}%) ó°–© ",
    "format-ethernet": "ğŸŒ {ipaddr}/{cidr} ",
    // "tooltip-format-wifi": "ğŸ›œ {essid}({signalStrength}%)",
    "tooltip-format-wifi": "ó°–©  {essid}({signalStrength}%)",
    "tooltip-format-ethernet": "ğŸŒ {ipaddr}/{cidr}",
    // "tooltip-format-disconnected": "â€¼ï¸ Disconnected",
    "tooltip-format-disconnected": "ó°–ª ",
    "on-click-right": "nm-connection-editor",
    "interval": 2
  },
  "hyprland/workspaces": {
    "disable-scroll": false,
    "on-scroll-up": "hyprctl dispatch workspace -1",
    "on-scroll-down": "hyprctl dispatch workspace +1",
    "format": "{name}", // {icon}
    // "on-click": "activate",
    "format-icons": {
      "urgent": "ó°—–",
      "active": "ó°¥",
      "default": "ó°¦"
    },
    "sort-by-number": true
  },
  "hyprland/window": {
    "format": " ïŒƒ {}",
    "separate-outputs": true,
    "max-length": 32,
    "rewrite": {
      "(.*)fish": "> [$1]",
      "(.*)neovide": "neovide ó°•· ", // doesn't seem to work
      "(.*)Mozilla Firefox": "Firefox ó°ˆ¹",
      "(.*)BlueMail": "BlueMail ó°Š« ",
      "(.*)Visual Studio Code": "Code ó°¨",
      "(.*)Dolphin": "$1 ó°‰‹",
      "(.*)Spotify": "Spotify ó°“‡",
      "(.*)Steam": "Steam ó°““"
    }
  },
  "persistent-workspaces": {
    "*": 5 // 5 workspaces by default on every monitor
  },
  "pulseaudio": {
    "format": "{icon} {volume}",
    "format-bluetooth": "{icon} ïŠ” {volume}%",
    "format-bluetooth-muted": "ó°Ÿ {icon}ïŠ”",
    "format-muted": "ïª€ {volume}",
    "tooltip-format": "{icon} {desc} // {volume}%",
    "scroll-step": 5,
    "format-icons": {
      "headphone": "ï€¥",
      "hands-free": "ï€¥",
      "headset": "ï€¥",
      "phone": "ï‚•",
      "portable": "ï‚•",
      "car": "ï†¹",
      "default": ["ï€¦", "ï€§", "ï€¨"]
    },
    "on-click": "pavucontrol"
    // "on-click-right": "blueman-manager"
  },
  "backlight": {
    // "device": "acpi_video1",
    "format": "{icon} {percent}%",
    // "format-icons": ["ğŸ”…", "ğŸ”†", "â˜€ï¸"],
    // "format-alt": "{percent}% {icon}",
    // "format-alt-click": "click-right",
    // "format": "{icon} {percent}%",
    // "format-icons": ["î", "î“", "î‘", "î", "î", "î", "îŠ", "îˆ", "î›"],
    // "on-scroll-up": "light -A 1",
    // "on-scroll-down": "light -U 1",
    "interval": 2,
    "format-icons": ["ó°¹‡", "ó°ƒœ", "ó°ƒ›", "ó°ƒ", "ó°ƒŸ", "ó°ƒ "],
    "states": {
      "normal": 0,
      "warning": 80,
      "critical": 90
    },
    "on-click": "hyprshade toggle bluefilter",
    "on-click-right": "hyprshade toggle extradark",
    "on-scroll-down": "brightnessctl -q set 5%-",
    "on-scroll-up": "brightnessctl -q set 5%+",
    "tooltip": false
  },
  "upower": {
    "icon-size": 18,
    "hide-if-empty": true,
    "tooltip": false,
    "tooltip-spacing": 20,
    "tooltip-padding": 8,
    "on-click": "cpupower-gui"
  },
  "custom/notifications": {
    "tooltip": false,
    "format": "{icon}",
    "format-icons": {
      "notification": "ï‚¢ <span foreground='red'><sup>ï‘„</sup></span>",
      // "notification": "ğŸ”” <span foreground='red'><sup>ï‘„</sup></span>",
      // "none": "ğŸ””",
      "none": "ï‚¢",
      "dnd-notification": "ï‚¢<span foreground='red'><sup>ï‘„</sup></span>",
      "dnd-none": "ï‚¢"

      // "dnd-notification": "ğŸ”• <span foreground='red'><sup>ï‘„</sup></span>"
      // "dnd-none": "ğŸ”•"
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true
  },
  "tray": {
    "icon-size": 15,
    "spacing": 15
  },
  "clock": {
    "format": "{:ó°¥” %H:%M ó°ƒ­ %d %b %Y}",
    "format-alt": "{:ïº %I:%M %p ï„³ %a %d}",
    //"format": "{:ğŸ•°ï¸ %R  ğŸ“… %d/%m}",
    //"format-alt": "{:%A, %B %d, %Y (%R)} ğŸ—“ï¸ ",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "weeks-pos": "right",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },
  "custom/weather": {
    "format": "{}",
    "on-click": "wpaperd",
    "tooltip": true,
    "restart-interval": 300,
    "exec": "$HOME/.config/waybar/scripts/waybar-wttr.py",
    "return-type": "json"
  },
  "custom/power": {
    "format": "{}",
